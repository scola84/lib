  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "overrides": [
      {
        "extends": [
          "eslint:all",
          "standard",
          "./.eslint/js.json"
        ],
        "files": [
          "**/*.js"
        ]
      },
      {
        "extends": [
          "eslint:all",
          "standard",
          "plugin:@typescript-eslint/all",
          "standard-with-typescript",
          "./.eslint/js.json",
          "./.eslint/ts.json"
        ],
        "files": [
          "**/*.ts"
        ]
      }
    ]
  },
  "files": [
    ".docker",
    ".eslint",
    "dist",
    "rollup",
    "types"
  ],
  "license": "MIT",
  "main": "./dist/server/index.js",
  "name": "@scola/lib",
  "prettier": {
    "htmlWhitespaceSensitivity": "ignore",
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "semi": false,
    "trailingComma": "none"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "preset": "angular"
  },
  "repository": "github:scola84/lib",
  "scripts": {
    "exec": "onchange --initial --kill ./dist/server/index.js -- node ./dist/server/index.js",
    "lint": "tsc --project ./src/client && tsc --project ./src/server && tsc --project ./test/server && lit-analyzer ./src/client --strict && eslint ./bin ./src ./test",
    "make": "rollup --config ./rollup/config.js",
    "test": "jest --preset ts-jest --runInBand"
  },
  "types": "./types/index.d.ts",
  "version": "25.3.0"
}
